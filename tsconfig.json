{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2017",
		"module": "amd",
		"moduleResolution": "node",
		"strict": true,
		"noImplicitReturns": true,
		"pretty": true,
		"lib": [
			"es2017",
			"dom",
			"dom.iterable"
		]
	},
	"include": [
		"src/**/*",
		"typings/**/*",
		"node_modules/@types/**/*"
	]
}